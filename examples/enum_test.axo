// Test fast enum-based operators
var a: int = 10;
var b: int = 5;

// Test arithmetic operators (now using fast enum comparisons)
var sum: int = a + b;
var diff: int = a - b;
var prod: int = a * b;
var quot: int = a / b;
var mod: int = a % b;

print("Sum:", sum);
print("Diff:", diff);
print("Product:", prod);
print("Quotient:", quot);
print("Modulo:", mod);

// Test comparison operators (now using fast enum comparisons)
print("a < b:", a < b);
print("a > b:", a > b);
print("a <= b:", a <= b);
print("a >= b:", a >= b);
print("a == b:", a == b);
print("a != b:", a != b);

// Test logical operators
var x: bool = true;
var y: bool = false;
print("x && y:", x && y);
print("x || y:", x || y);
print("!x:", !x);

// Test in a loop (where performance matters most)
var total: int = 0;
for (var i: int = 0; i < 1000; i = i + 1) {
    total = total + i;
}
print("Loop total:", total);