var sum: int = 0;

// Simple for loop that should be JIT compiled
for (var i: int = 0; i < 1000000; i = i + 1) {
    sum = sum + 1;
}

print("Sum:", sum);

var count: int = 0;
var j: int = 0;

// Simple while loop that should be JIT compiled  
while (j < 500000) {
    count = count + 2;
    j = j + 1;
}

print("Count:", count);