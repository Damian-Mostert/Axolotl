// Mathematical functions
func square(n: int) -> int {
    return n * n;
}

func factorial(n: int) -> int {
    if (n <= 1) {
        return 1;
    }
    return n * factorial(n - 1);
}

func add(a: int, b: int) -> int {
    return a + b;
}

func main() -> void {
    var result1: int = square(5);
    var result2: int = factorial(5);
    var result3: int = add(10, 20);
}
